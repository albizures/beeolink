{
	"name": "beeolink",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint .",
		"test:types": "tsc --noEmit",
		"db:generate": "drizzle-kit generate:sqlite",
		"db:push": "drizzle-kit push:sqlite",
		"pages:dev": "wrangler pages dev .vercel/output/static --compatibility-date=2024-02-08 --compatibility-flag=nodejs_compat",
		"pages:build": "npx @cloudflare/next-on-pages",
		"pages:preview": "npm run pages:build && wrangler pages dev .vercel/output/static --compatibility-date=2024-02-08 --compatibility-flag=nodejs_compat",
		"pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static"
	},
	"dependencies": {
		"@auth/core": "^0.28.0",
		"@auth/drizzle-adapter": "^0.8.0",
		"@libsql/client": "^0.5.2",
		"@t3-oss/env-nextjs": "^0.9.2",
		"@vyke/dency": "^0.0.9",
		"@vyke/results": "^0.2.3",
		"@vyke/sola": "^0.0.6",
		"clsx": "^2.1.0",
		"crypto": "^1.0.1",
		"drizzle-orm": "^0.29.4",
		"next": "14.1.2",
		"next-auth": "^4.24.6",
		"react": "^18",
		"react-dom": "^18",
		"react-icons": "^5.0.1",
		"tailwind-merge": "^2.2.1",
		"uuid": "^9.0.1",
		"zod": "^3.22.4",
		"zustand": "^4.5.2"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^2.8.0",
		"@cloudflare/next-on-pages": "^1.10.0",
		"@cloudflare/workers-types": "^4.20240222.0",
		"@eslint/eslintrc": "^3.0.2",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@types/uuid": "^9.0.8",
		"autoprefixer": "^10.4.18",
		"daisyui": "^4.7.2",
		"drizzle-kit": "^0.20.14",
		"eslint": "^8",
		"eslint-config-next": "14.1.2",
		"eslint-plugin-next-on-pages": "^1.10.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"postcss": "^8",
		"tailwindcss": "^3.4.1",
		"type-fest": "^4.11.1",
		"typescript": "^5",
		"vercel": "^33.5.3",
		"wrangler": "^3.31.0"
	},
	"browser": {
		"crypto": false,
		"stream": false
	}
